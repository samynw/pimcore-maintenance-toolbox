{
    "name": "samynw/pimcore-maintenance-toolbox",
    "description": "This provides a set of usefull tools for the Pimcore maintenance",
    "type": "pimcore-bundle",
    "require": {
        "pimcore/pimcore": "^6.8.0 || ^10.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Wouter Samyn",
            "role": "Developer"
        }
    ],
    "autoload": {
      "psr-4": {
        "Samynw\\MaintenanceToolboxBundle\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Samynw\\MaintenanceToolboxBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "Samynw\\MaintenanceToolboxBundle\\MaintenanceToolboxBundle"
            ]
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "symfony/phpunit-bridge": "5.x-dev",
        "phpunit/phpunit": "^9.5@dev",
        "mockery/mockery": "^1.4@dev"
    }
}
